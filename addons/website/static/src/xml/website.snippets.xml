                    </div>
                </div>
            </div>
            <!-- Carousel nav -->
            <a class="carousel-control left" href="#myCarousel" data-slide="prev" style="width: 10%"></a>
            <a class="carousel-control right" href="#myCarousel" data-slide="next" style="width: 10%"></a>
        </div>
    </div>

