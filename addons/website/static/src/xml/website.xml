<?xml version="1.0" encoding="UTF-8"?>
<!-- vim:fdl=1:
-->
<templates id="template" xml:space="preserve">
<t t-name="Website.EditorBar">
    <ul class="oe_website_editorbar openerp">
        <li><button data-action="edit" type="button">Edit</button></li>
        <li><button data-action="save" type="button">Save</button></li>
        <li><button data-action="cancel" type="button">Cancel</button></li>
        <li class="oe_right"><button data-action="snippet" type="button"
            >Building Blocks</button></li>
    </ul>
</t>
<t t-name="Website.ActionGroup">
    <div class="dropdown">
        <button type="button" class="oe_button_list dropdown-toggle">
            <t t-esc="widget.name"/>
        </button>
        <ul class="dropdown-menu" role="menu"/>
    </div>
</t>
<t t-name="Website.Snipped.carousel">
    <div class="item" style="background-image: url(/website/static/src/img/china.jpg); background-size: cover; background-position: bottom;" >
        <div class='container'>
            <h1 class='mt64'>Your New Banner Slide</h1>
            <h2>With another convincing subtitle</h2>
        </div>
    </div>
</t>
</templates>
