<?xml version="1.0" encoding="UTF-8"?>
<!-- vim:fdl=1:
-->
<templates id="template" xml:space="preserve">
<t t-name="Website.EditorBar">
    <ul class="oe_website_editorbar openerp">
        <li><button data-action="edit">Edit</button></li>
        <li><button data-action="save">Save</button></li>
        <li><button data-action="cancel">Cancel</button></li>
        <li class="oe_right"><button data-action="snippet">Building Blocks</button></li>
    </ul>
</t>
<t t-name="Website.ActionGroup">
    <div class="dropdown">
        <button type="button" class="oe_button_list dropdown-toggle">
            <t t-esc="widget.name"/>
        </button>
        <ul class="dropdown-menu" role="menu"/>
    </div>
</t>
<t t-name="Website.Snipped.carousel">
    <div style="position: absolute; top: -4px;">
        <span class="label" style="cursor:pointer"><i class="add icon-plus-sign"></i></span>
        <span class="label" style="cursor:pointer"><i class="remove icon-minus-sign"></i></span>
        <div style="display: none;">
            <div class="item">
                <img src="/website/static/src/img/volcano.jpg"/>
                <div class="css_titlebox" style="position: absolute; top: 25px; right: 80px;">
                    <div style="text-align: right; background: #f6f6f6; padding: 7px 20px; border-radius: 5px;">
                        <h1>Insert Title Here</h1>
                        <h4>Insert subtitle here</h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
</t>
</templates>
