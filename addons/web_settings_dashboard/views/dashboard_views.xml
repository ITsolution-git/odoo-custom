<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="web_settings_dashboard_action" model="ir.actions.client">
        <field name="name">Dashboard</field>
        <field name="tag">web_settings_dashboard.main</field>
    </record>
    <menuitem name="Dashboard" id="web_dashboard_menu" action="web_settings_dashboard_action" parent="base.menu_administration" sequence="-1" groups="base.group_system"/>

    <!-- Make the xmlid of menus required by the Website Generic tour available in webclient -->
    <record id="base.menu_administration" model="ir.ui.menu">
        <field name="load_xmlid" eval="True"/>
    </record>

</odoo>
