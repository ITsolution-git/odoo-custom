msgid ""
"\n"
"Hello${object.partner_order_id.name and ' ' or "
"''}${object.partner_order_id.name or ''},\n"
"\n"
"Here is your order confirmation for ${object.partner_id.name}:\n"
"       | Order number: *${object.name}*\n"
