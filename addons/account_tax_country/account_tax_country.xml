<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>


		<record model="ir.ui.view" id="account_country_tax_form_view">
			<field name="name">account.tax.country.form</field>
			<field name="model">account.tax</field>
 			<field name="type">form</field>			
			<field name="inherit_id" ref="account.view_tax_form"/>
			<field name="arch" type="xml">
	                <xpath expr="/form/notebook/page[@string='Special Computation']" position="after">	
	                	<page string="Country">		
							<field name="country_id" nolabel="1"/>
						</page>
					</xpath>
			</field>
		</record>
		
		
    </data>
</openerp>            