<?xml version="1.0"?>
<openerp>
<data noupdate="1">

    <record model="mrp_operations.operation.code" id="mrp_op_1">
        <field name="name">Start</field>
        <field name="code">start</field>
        <field name="start_stop">start</field>
    </record>

    <record model="mrp_operations.operation.code" id="mrp_op_2">
        <field name="name">Pause</field>
        <field name="code">pause</field>
        <field name="start_stop">pause</field>
    </record>

    <record model="mrp_operations.operation.code" id="mrp_op_3">
        <field name="name">Cancel</field>
        <field name="code">cancel</field>
        <field name="start_stop">cancel</field>
    </record>
    <record model="mrp_operations.operation.code" id="mrp_op_4">
        <field name="name">Resume</field>
        <field name="code">resume</field>
        <field name="start_stop">resume</field>
    </record>
    <record model="mrp_operations.operation.code" id="mrp_op_5">
        <field name="name">Finished</field>
        <field name="code">done</field>
        <field name="start_stop">done</field>
    </record>

    <!-- SUBTYPE -->
