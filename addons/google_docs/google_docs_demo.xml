<?xml version="1.0"?>
<openerp>
    <data noupdate="1">

<!-- filter demo  -->    
      <record id="filter_crm" model="ir.filters">
          <field name="name">Sales Team</field>
          <field name="model_id">crm.lead</field>
          <field name="is_default">False</field>
          <field name="domain">[['section_id', 'ilike', 'sales department']]</field>
          <field name="context">{'lang': 'en_US', 'tz': 'Europe/Brussels', 'uid': 1}</field>
      </record>    

<!-- template demo -->
      <record id="template_crm" model="google.docs.config">
          <field name="model_id" ref="crm.model_crm_lead"/>
          <field name="filter_id" ref="filter_crm"/>
          <field name="gdocs_template_url">https://docs.google.com/spreadsheet/ccc?key=0Ah2qnrLAoZmUdGRvdVdmS1VoSDctWk1kd18taGZ4ckE#gid=0</field>
          <field name="name_template">lead_sales-department_%(name)s_review</field>
      </record>
      
    </data>
</openerp>

