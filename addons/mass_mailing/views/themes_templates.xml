                                        <a t-att-href="website.social_facebook">
                                          <span class="fa fa-facebook"></span>
                                        </a>
                                    </t>
                                    <t t-if="website.social_googleplus">
                                        <a t-att-href="website.social_googleplus" style="margin-left:10px">
