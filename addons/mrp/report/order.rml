    </section>
    <para style="P7">
      <font color="white"> </font>
    </para>
    <para style="P8">
      <font color="white"> .........</font>
      <font color="white"> .........</font>
    </para>
    <blockTable colWidths="86.0,16.0,425.0" style="Table3">
      <tr>
        <td>
          <para style="P15">Planned Date </para>
        </td>
        <td>
          <para style="P16">:</para>
        </td>
        <td>
          <para style="P15">[[ time.strftime('%d/%m/%Y', time.strptime(o.date_planned, '%Y-%m-%d %H:%M:%S')) ]]</para>
