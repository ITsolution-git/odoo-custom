<?xml version="1.0" encoding="ISO-8859-1"?>
<achat-listing>
	<date type="function" name="today"></date>
	<subject>Listing des acheteurs</subject>
	<auctions type="fields" name="auction_id">
		<title type="field" name="auction_id.name"/>
		<cost-index type="call" name="" args="">
			<id value="id"/>
			<name value="name"/>
	</cost-index>
		<buyers type="fields" name="ach_login">
			<id type="field" name="id"/>
			<login type="field" name="ach_login"/>
			<name type="field" name="ach_uid.name"/>
			<product type="fields" name="obj_num,id">
			<cost type="call" name="" args="">
					<id value="id"/>
					<amount value="amount"/>
				</cost>
				<ref type="field" name="obj_num"/>
				<price type="field" name="obj_price" default="0.0"/>
				<paid type="field" name="state"/>
				<payment type="field" name="ach_pay_id.id"/>
			</product>
		</buyers>
	</auctions>
</achat-listing>
