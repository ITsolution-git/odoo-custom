<?xml version="1.0" encoding="ISO-8859-1"?>
<deposit-form>
	<date type="function" name="today"/>
	<auction-date type="field" name="auction_id.auction1"/>
	<deposit type="fields" name="bord_vnd_id">
		<deposit-infos type="zoom" name="bord_vnd_id">
			<deposit-to type="zoom" name="partner_id">
				<corporation>
					<title type="field" name="title"/>
					<name type="field" name="name"/>
					<vat type="field" name="vat"/>
				</corporation>
				<person type="zoom" name="address">
					<title type="field" name="title"/>
					<name type="field" name="name"/>
					<street type="field" name="street"/>
					<street2 type="field" name="street2"/>
					<postcode type="field" name="zip"/>
					<city type="field" name="city"/>
					<!--<state type="field" name="state.name"/>-->
					<!--<country type="field" name="country.name"/>-->
				</person>
			</deposit-to>
		</deposit-infos>
		<deposit-lines>
			<deposit-line type="fields" name="lot_num,id">
				<obj-num type="field" name="obj_num"/>
				<bord-vnd-id type="field" name="bord_vnd_id.name"/>
				<lot-num type="field" name="lot_num"/>
				<name type="field" name="name"/>
				<artist type="field" name="artist_id.name"/>
				<lot-est1 type="field" name="lot_est1"/>
				<lot-est2 type="field" name="lot_est2"/>
			</deposit-line>
		</deposit-lines>
	</deposit>
</deposit-form>
