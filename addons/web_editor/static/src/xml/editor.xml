<templates id="template" xml:space="preserve">
    <t t-name="web_editor.editorbar">
        <div class="o_css_editor navbar navbar-fixed-top hidden-xs" id="web_editor-top-edit">
            <div id="web_editor-toolbars" />
            <form class="navbar-form text-muted">
                <div id="web_editor-top-actions">
                    <a class="btn btn-link btn-xs" data-action="cancel" href="#">
                        <i class="material-icons">clear</i>DISCARD</a>
                    <button class="btn full compl btn-xs" data-action="save" type="button">
                        <i class="fa fa-floppy-o"></i>  SAVE</button>
                </div>
            </form>
        </div>
    </t>
    <t t-name="web_editor.dialog">
        <div aria-hidden="true" class="modal web_editor-dialog" role="dialog" tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button aria-hidden="true" class="close" data-dismiss="modal" type="button">×</button>
                        <h3 class="modal-title">
                            <t t-esc="title"/>
                        </h3>
                    </div>
                    <div class="modal-body">
                        <t t-raw="0"/>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-primary save" t-if="!button" type="button">Save</button>
                        <button class="btn btn-primary save" t-if="button" type="button">
                            <t t-raw="button"/>
                        </button>
                        <button class="btn hidden wait" disabled="disabled" type="button"/>
                        <span>or </span>
                        <a aria-hidden="true" data-dismiss="modal" href="#">Discard</a>
                    </div>
                </div>
            </div>
        </div>
    </t>
    <t t-name="web_editor.dialog.background_position">
        <div aria-hidden="true" class="modal website_modal web_editor-background_position" role="dialog" tabindex="-1">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button aria-hidden="true" class="close" data-dismiss="modal" type="button">×</button>
                        <h3 class="modal-title">Background Image Options</h3>
                    </div>
                    <div class="modal-body">
                        <div class="panel-group" id="bg_image_opt_accordition" role="tablist" aria-multiselectable="true">
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="heading_cover">
                                    <label role="button" class="radio inline"  data-toggle="collapse" data-parent="#bg_image_opt_accordition" href="#collapse_cover">
                                        <input id="radio1" class="opt_select" name="rdo" type="radio"/>COVER</label>
                                </div>
                                <div class="panel-collapse collapse" role="tabpanel" id="collapse_cover">
                                    <div class="panel-body">

                                        <a class="help-controll collapsed" role="button" data-toggle="collapse" href="#cover_help" aria-expanded="false" aria-controls="cover_help"><i class="material-icons">help</i></a>

                                        <div class="help collapse" id="cover_help">

                                            <div class="simulator cover_bg">
                                                <div class="bg"></div>
                                                <div class="el"></div>
                                            </div>

                                            <div class="help-text">
                                                <p>The 'cover' method scale the background image to be as large as possible so that the background area is completely covered by the background image. Some parts of the background image may not be in view</p>
                                            </div>
                                        </div>
                                        <p><strong>Assigning a focal point to ensure that the targeted area will be always visible</strong></p>
                                        <div class="cover_editing_container">
                                            <h6>Click on the picture</h6>
                                            <div class="object">
                                                <span class="grid grid-1"/>
                                                <span class="grid grid-2"/>
                                                <span class="grid grid-3"/>
                                                <span class="grid grid-4"/>
                                                <span class="focus_point"/>
                                                <div class="ui_info">X: <span class="x"/> Y: <span class="y"/></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="heading_contain">
                                    <label role="button" class="radio inline"  data-toggle="collapse" data-parent="#bg_image_opt_accordition" href="#collapse_contain">
                                        <input id="radio2" class="opt_select" name="rdo" type="radio"/>CONTAIN</label>
                                </div>
                                <div class="panel-collapse collapse" role="tabpanel" id="collapse_contain">
                                    <div class="panel-body">
                                        <a class="help-controll collapsed" role="button" data-toggle="collapse" href="#contain_help" aria-expanded="false" aria-controls="contain_help"><i class="material-icons">help</i></a>

                                        <div class="help collapse" id="contain_help">

                                            <div class="simulator contain_bg">
                                                <div class="bg"></div>
                                                <div class="el"></div>
                                            </div>

                                            <div class="help-text">
                                                <p>Scale the image to the largest size such that both its width and its height can fit inside the content area</p>
                                            </div>
                                        </div>

                                        <form class="form-horizontal">
                                          <div class="form-group">
                                            <label class="col-sm-2 control-label" for="repeatInput">Repeat</label>
                                            <div class="col-sm-5">
                                                <div class="checkbox">
                                                    <label>
                                                      <input id="contain_repeat"  type="checkbox"/>
                                                    </label>
                                                </div>
                                            </div>
                                          </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="heading_custom">
                                    <label role="button" class="radio inline"  data-toggle="collapse" data-parent="#bg_image_opt_accordition" href="#collapse_custom">
                                        <input id="radio3" class="opt_select" name="rdo" type="radio"/>CUSTOM</label>
                                </div>
                                <div class="panel-collapse collapse" role="tabpanel" id="collapse_custom">
                                    <div class="panel-body">
                                        <form class="form-horizontal">
                                            <fieldset>
                                                <legend>Background size</legend>
                                                <p>Sets the width and height of the background image in percent of the parent element.</p>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label" for="custom_size_x">Background width</label>
                                                    <div class="col-sm-5">
                                                        <div class="input-group">
                                                            <input type="number" min="0" max="100" class="form-control input-sm" id="custom_size_x" placeholder="auto"/>
                                                            <div class="input-group-addon">%</div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label" for="custom_size_y">Background height</label>
                                                    <div class="col-sm-5">
                                                        <div class="input-group">
                                                            <input type="number" min="0" max="100" class="form-control input-sm" id="custom_size_y" placeholder="auto"/>
                                                            <div class="input-group-addon">%</div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <legend>Background position</legend>
                                                <p>Set the starting position of the background image.</p>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label" for="custom_pos_x">Horizontal</label>
                                                    <div class="col-sm-5">
                                                        <div class="input-group">
                                                            <input type="number" min="0" max="100" class="form-control input-sm" id="custom_pos_x" placeholder="auto"/>
                                                            <div class="input-group-addon">%</div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label" for="custom_pos_x">Vertical</label>
                                                    <div class="col-sm-5">
                                                        <div class="input-group">
                                                            <input type="number" min="0" max="100" class="form-control input-sm" id="custom_pos_y" placeholder="auto"/>
                                                            <div class="input-group-addon">%</div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <fieldset>
                                                <legend>Background repeat</legend>
                                                <p>Define if/how the background image will be repeated</p>
                                                <select class="form-control" id="custom_repeat">
                                                    <option value="o_bg_img_no_repeat">No repeat</option>
                                                    <option value="o_bg_img_repeat">Repeat both</option>
                                                    <option value="o_bg_img_repeat_x">Repeat x</option>
                                                    <option value="o_bg_img_repeat_y">Repeat y</option>
                                                </select>
                                            </fieldset>
                                        </form>

                                    </div>
                                </div>
                            </div> <!-- / .panel -->
                        </div> <!-- / .panel-group -->
                        <input id="pos_x" type="hidden"/>
                        <input id="pos_y" type="hidden"/>
                        <input id="bg_size" type="hidden"/>
                    </div> <!-- /.modal-body -->

                    <div class="modal-footer">
                        <button class="btn btn-primary save" data-dismiss="modal" type="button">Save</button> or <a aria-hidden="true" data-dismiss="modal" href="#">Discard</a>
                    </div>

                </div>
            </div>
        </div>
    </t>
