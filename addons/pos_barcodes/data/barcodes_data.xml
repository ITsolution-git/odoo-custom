<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	<record id="default_barcode_nomenclature" model="barcode.nomenclature">
            <field name="name">Default Nomenclature</field>
        </record>

        <record id="barcode_rule_product" model="barcode.rule">
            <field name="name">Product Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">60</field>
            <field name="type">product</field>
            <field name="pattern">*</field>
        </record>

        <record id="barcode_rule_cashier" model="barcode.rule">
            <field name="name">Cashier Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">50</field>
            <field name="type">cashier</field>
            <field name="pattern">041</field>
        </record>

        <record id="barcode_rule_client" model="barcode.rule">
            <field name="name">Client Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">40</field>
            <field name="type">client</field>
            <field name="pattern">042</field>
        </record>

        <record id="barcode_rule_weight" model="barcode.rule">
            <field name="name">Weight Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">30</field>
            <field name="type">weight</field>
            <field name="pattern">21*****NNDDD*</field>
        </record>

        <record id="barcode_rule_discount" model="barcode.rule">
            <field name="name">Discount Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">20</field>
            <field name="type">discount</field>
            <field name="pattern">22********NN*</field>
        </record>

        <record id="barcode_rule_price" model="barcode.rule">
            <field name="name">Price Barcodes</field>
            <field name="barcode_nomenclature_id" ref="default_barcode_nomenclature"/>
            <field name="sequence">10</field>
            <field name="type">price</field>
            <field name="pattern">23*****NNNDD*</field>
        </record>
    </data>
</openerp>		